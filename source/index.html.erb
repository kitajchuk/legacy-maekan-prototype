---
title: Feed
topic: Everything
controller: app
---
<%
@collection = ordered_random_merge( data.feed, data.links )
@collection = ordered_random_merge( @collection, data.features )
%>
<%= partial "partials/cover.erb" %>


<%= partial "partials/refine-label.erb" %>


<section class="feed feed--center js-feed" data-page="0" data-pages="5">
    <% @collection.each do |item| %>
        <% if item.linklist %>
            <%= partial "partials/feeditem-linklist", :locals => { :item => item } %>
        <% else %>
            <%= partial "partials/feeditem", :locals => { :item => item } %>
        <% end %>
    <% end %>
</section>